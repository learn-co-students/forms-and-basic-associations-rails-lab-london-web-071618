<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name, list: "artists_autocomplete" %>
  <datalist id="artists_autocomplete">
    <% Artist.all.each do |artist| %>
      <option value="<%=artist.name%>">
        <%end%>
  </datalist>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <input name="song[notes][]" id="song_notes_1">
  <input name="song[notes][]" id="song_notes_2">
  <br/>

  <%= f.submit %>
<% end %>

